  "query": "\n        SELECT\n            id,\n            user_id,\n            hash,\n            hash_type,\n            expiry_time,\n            used,\n            metadata,\n            created_at\n        FROM activation_hashes\n        WHERE hash = $1\n          AND hash_type = $2\n          AND user_id = $3\n          AND used = 0\n          AND expiry_time > NOW()\n        LIMIT 1\n        ",
