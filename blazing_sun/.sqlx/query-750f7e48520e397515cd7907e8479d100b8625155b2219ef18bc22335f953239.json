{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            cea.client_id,\n            cea.api_product_id,\n            ap.product_key,\n            ap.product_name,\n            ap.product_description,\n            cea.created_at\n        FROM oauth_client_enabled_apis cea\n        JOIN oauth_api_products ap ON cea.api_product_id = ap.id\n        WHERE cea.client_id = $1\n        ORDER BY ap.product_name\n        ",
