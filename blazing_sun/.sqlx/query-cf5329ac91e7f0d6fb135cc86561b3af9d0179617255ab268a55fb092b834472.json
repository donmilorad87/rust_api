  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            l.id as locale_id,\n            l.locale_code,\n            lt.singular,\n            lt.plural\n        FROM localization_translations lt\n        INNER JOIN locales l ON l.id = lt.locale_id\n        WHERE lt.localization_key_id = $1\n        ORDER BY l.locale_code ASC\n        ",
