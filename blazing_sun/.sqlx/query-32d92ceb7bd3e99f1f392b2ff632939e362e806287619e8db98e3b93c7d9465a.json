{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            g.id,\n            g.gallery_uuid,\n            g.name,\n            g.description,\n            g.latitude,\n            g.longitude,\n            g.tags,\n            g.cover_image_id,\n            g.cover_image_uuid,\n            COUNT(p.id) as \"picture_count!\"\n        FROM galleries g\n        LEFT JOIN pictures p ON g.id = p.gallery_id\n        WHERE g.gallery_type = 'geo_galleries'\n            AND g.latitude IS NOT NULL\n            AND g.longitude IS NOT NULL\n        GROUP BY g.id\n        ORDER BY g.created_at DESC\n        ",
