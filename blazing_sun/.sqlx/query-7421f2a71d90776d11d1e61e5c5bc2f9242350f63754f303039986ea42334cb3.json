{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            p.id,\n            p.name,\n            p.place_type,\n            p.description,\n            p.latitude,\n            p.longitude,\n            p.created_by,\n            p.created_at,\n            p.updated_at,\n            COUNT(i.id) as \"image_count!\"\n        FROM geo_places p\n        LEFT JOIN geo_place_images i ON p.id = i.place_id\n        GROUP BY p.id\n        ORDER BY p.created_at DESC\n        ",
