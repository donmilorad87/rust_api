  "query": "\n        SELECT\n            id,\n            token_hash,\n            token_hint,\n            user_id,\n            device_info,\n            ip_address,\n            is_revoked,\n            revoked_at,\n            expires_at,\n            last_used_at,\n            created_at\n        FROM session_refresh_tokens\n        WHERE user_id = $1\n          AND is_revoked = FALSE\n          AND expires_at > NOW()\n        ORDER BY created_at DESC\n        ",
