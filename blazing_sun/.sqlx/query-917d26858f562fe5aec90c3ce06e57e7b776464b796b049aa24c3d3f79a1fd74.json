{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            ps.id,\n            ps.page_seo_id,\n            ps.lang_code,\n            ps.schema_type,\n            ps.entity_schema_id,\n            se.schema_data,\n            ps.position,\n            ps.is_active,\n            ps.created_at,\n            ps.updated_at\n        FROM page_schemas ps\n        LEFT JOIN schema_entities se\n            ON se.lang_code = ps.lang_code\n            AND se.schema_id = ps.entity_schema_id\n        WHERE ps.page_seo_id = $1 AND ps.lang_code = $2\n        ORDER BY ps.position ASC, ps.id ASC\n        ",
