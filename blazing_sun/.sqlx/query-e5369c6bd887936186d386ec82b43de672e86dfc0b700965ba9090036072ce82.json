{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            cas.client_id,\n            cas.scope_id,\n            sc.scope_name,\n            sc.scope_description,\n            sc.sensitive,\n            cas.created_at\n        FROM oauth_client_allowed_scopes cas\n        JOIN oauth_scope_catalog sc ON cas.scope_id = sc.id\n        WHERE cas.client_id = $1\n        ORDER BY sc.scope_name\n        ",
