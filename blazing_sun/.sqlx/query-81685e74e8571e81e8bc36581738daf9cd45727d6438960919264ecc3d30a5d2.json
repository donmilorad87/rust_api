{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            g.id,\n            g.user_id,\n            g.name,\n            g.description,\n            g.is_public,\n            g.display_order,\n            g.cover_image_id,\n            g.cover_image_uuid,\n            g.created_at,\n            g.updated_at,\n            COUNT(p.id) as \"picture_count!\"\n        FROM galleries g\n        LEFT JOIN pictures p ON g.id = p.gallery_id\n        WHERE g.is_public = true\n        GROUP BY g.id\n        ORDER BY g.created_at DESC\n        LIMIT $1 OFFSET $2\n        ",
