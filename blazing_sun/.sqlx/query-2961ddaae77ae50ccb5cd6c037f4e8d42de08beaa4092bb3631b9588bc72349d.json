{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            i.id,\n            i.place_id,\n            i.upload_id,\n            u.uuid as \"upload_uuid!\",\n            i.title,\n            i.description,\n            i.tag,\n            i.latitude,\n            i.longitude,\n            i.created_by,\n            i.created_at,\n            i.updated_at\n        FROM geo_place_images i\n        JOIN uploads u ON u.id = i.upload_id\n        WHERE i.place_id = $1\n        ORDER BY i.created_at DESC\n        ",
